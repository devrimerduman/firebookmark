{
  "manifest_version": 2,
  "name": "Bookmark Organizer",
  "version": "1.0",
  "description": "Firefox bookmarklarını yedekler ve içeriklerine göre yeniden organize eder",
  "permissions": [
    "bookmarks",
    "downloads",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Bookmark Organizer",
    "default_popup": "popup.html"
  }
} 